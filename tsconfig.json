{
  "compilerOptions": {
    "target": "ES5",
    "module": "UMD",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "baseUrl": "./src",
    "outDir": "./dist",
    "lib": ["ES2016"],
    "esModuleInterop": true
  },
  "exclude": ["dist", "node_modules", "tests"]
}
